os: windows
and app.name: Microsoft Visual Studio
os: windows
and app.exe: devenv.exe
--

build solution: key(ctrl-shift-b)
stop build: key(ctrl-break)
    

start debugging: key(f5)
continue: key(f5)
start without debugging: key(ctrl-f5)
stop debugging: key(shift-f5)

breakpoint: key(f9)

step over: key(f10)
step into: key(f11)
step out: key(shift-f11)

comment out: 
    key(ctrl-k)
    key(ctrl-c)

remove comment: 
    key(ctrl-k)
    key(ctrl-u)